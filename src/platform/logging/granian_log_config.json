{
  "version": 1,
  "disable_existing_loggers": false,
  "handlers": {
    "intercept": {
      "class": "src.platform.logging.loguru_io_config.InterceptHandler"
    }
  },
  "root": {
    "level": "INFO",
    "handlers": [
      "intercept"
    ]
  },
  "loggers": {
    "granian": {
      "level": "INFO",
      "handlers": [
        "intercept"
      ],
      "propagate": false
    },
    "granian.server": {
      "level": "INFO",
      "handlers": [
        "intercept"
      ],
      "propagate": false
    },
    "granian.access": {
      "level": "INFO",
      "handlers": [
        "intercept"
      ],
      "propagate": false
    }
  }
}